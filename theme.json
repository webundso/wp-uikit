{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version":2,
  "settings": {
    "appearanceTools": false,
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "layout": {
      "contentSize": "840px",
      "wideSize": "1100px"
    },
    "color": {
        "text": true,
        "background": true,
        "link": false,
        "custom": false,
        "customDuotone": false,
        "customGradient": false,
        "defaultDuotone": false,
        "defaultGradients": false,
        "defaultPalette": false,
        "palette": [
          {
            "slug": "black",
            "color": "#000000",
            "name": "schwarz"
          },  
          {
            "slug": "weiss",
            "color": "#ffffff",
            "name": "weiss"
          },  
          {
            "slug": "orange",
            "color": "#FF9900",
            "name": "orange"
          }
        ]
    },
